        <short>Dialog complexity for the runlevel-admin tool</short>
        <long>The GNOME System Tools have two data display modes: basic mode and advanced mode, the basic mode is intended to show less options than the advanced mode</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/runlevel/default_runlevel</key>
      <applyto>/apps/gnome-system-tools/runlevel/default_runlevel</applyto>
      <owner>gnome</owner>
      <type>int</type>
      <default>1</default>
      <locale name="C">
        <short>Default runlevel</short>
        <long>Runlevel that the runlevel-admin tool shows as default in basic mode</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/boot/advanced_mode</key>
      <applyto>/apps/gnome-system-tools/boot/advanced_mode</applyto>
      <owner>gnome</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Dialog complexity for the boot-admin tool</short>
        <long>The GNOME System Tools have two data display modes: basic mode and advanced mode, the basic mode is intended to show less options than the advanced mode</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/time/advanced_mode</key>
      <applyto>/apps/gnome-system-tools/time/advanced_mode</applyto>
      <owner>gnome</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Dialog complexity for the time-admin tool</short>
        <long>The GNOME System Tools have two data display modes: basic mode and advanced mode, the basic mode is intended to show less options than the advanced mode</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/users/advanced_mode</key>
      <applyto>/apps/gnome-system-tools/users/advanced_mode</applyto>
      <owner>gnome</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Dialog complexity for the users-admin tool</short>
        <long>The GNOME System Tools have two data display modes: basic mode and advanced mode, the basic mode is intended to show less options than the advanced mode</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/users/showall</key>
	 <applyto>/apps/gnome-system-tools/users/showall</key>
	 <owner>gnome</owner>
	 <type>bool</type>
	 <default>true</default>
	 <locale name="C">
        <short>Wether the users-admin tool should show all users and groups or not</short>
        <long></long>
	 </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/global/show_warning</key>
      <applyto>/apps/gnome-system-tools/global/show_warning</applyto>
      <owner>gnome</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>wether the warning dialog should be shown or not</short>
        <long></long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/global/last-version</key>
      <applyto>/apps/gnome-system-tools/global/last-version</applyto>
      <owner>gnome</owner>
      <type>string</type>
	 <default>0</default>
      <locale name="C">
        <short>Last GST version executed</short>
        <long>The last GST version that this computer has run</long>
      </locale>
