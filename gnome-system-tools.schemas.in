<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/gnome-system-tools/network/advanced_mode</key>
      <applyto>/apps/gnome-system-tools/network/advanced_mode</applyto>
      <owner>gnome</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Dialog complexity for the network-admin tool</short>
        <long>The GNOME System Tools have two data display modes: basic mode and advanced mode, the basic mode is intended to show less options than the advanced mode</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnome-system-tools/runlevel/advanced_mode</key>
      <applyto>/apps/gnome-system-tools/runlevel/advanced_mode</applyto>
